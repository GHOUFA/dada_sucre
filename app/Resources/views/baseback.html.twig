<div class="app-content content container-fluid">
    <div class="content-wrapper os-content">
        {#{%  include(':elements:header.html.twig') %}#}

        <div class="content-header row">
            <div class="content-header-right col-md-12 col-xs-12 mb-1">
                <div class="breadcrumb-wrapper col-xs-12">
                    <ol class="breadcrumb">
                        {% block breadcrumb %}
                            <li class="breadcrumb-item">
                                {#<a href="{{ path('agence_manager_homepage') }}">Tableau de bord</a>#}
                            </li>
                        {% endblock %}
                    </ol>
                </div>
            </div>
            <div class="content-header-right col-md-6 col-xs-12 mb-1">
                {% block flash_messages %}
                    {% for flashMessage in app.session.flashbag.get('success') %}
                        <div class="alert alert-success alert-dismissible fade in mb-2" role="alert">
                            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                <span aria-hidden="true">x</span>
                            </button>
                            <strong>Success</strong> {{ flashMessage }}.
                        </div>
                    {% endfor %}
                    {% for flashMessage in app.session.flashbag.get('error') %}
                        <div class="alert alert-danger alert-dismissible fade in mb-2" role="alert">
                            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                <span aria-hidden="true">x</span>
                            </button>
                            <strong>Erreur</strong> {{ flashMessage }}.
                        </div>
                    {% endfor %}
                {% endblock %}
            </div>
        </div>

        {% block body %}{% endblock %}
    </div>
</div>

